    <div class="modal inmodal fade" id="modalAdd" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content modal-lg">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h4 class="modal-title">Registrar un nuevo ticket</h4>

                </div>
                <div class="modal-body">
                    <form id="formAdd" method="POST" action="/ticket">


                        <fieldset class="form-horizontal">

                            <div class="row">
                                <div class="col-md-5">
                                    <div class="form-group"><label class="col-sm-3 control-label">Cliente:</label>
                                        <div class="col-sm-9">
                                            <select required id="Clients" class="form-control" name="client">
                                                <option w3-repeat="clients"  value="{{id}}">{{name}}</option>
                                            </select></div>
                                    </div>
                                    <div class="form-group"><label class="col-sm-3 control-label">Tipo:</label>
                                        <div class="col-sm-9">
                                            <select required class="form-control" name="type" id="type">
                                                <option value="success">SOPORTE</option>
                        
                                            </select>
                                        </div>
                                    </div>



                                </div>
                                <div class="col-md-7">
                                    <div class="form-group"><label class="col-sm-3 control-label">Prioridad:</label>
                                        <div class="col-sm-9">
                                            <select required class="form-control" name="priority" id="priority">
                                                    <option value="primary">BAJA</option>
                                                    <option value="warning">MEDIA</option>
                                                    <option value="danger">ALTA</option>
                                                </select>
                                        </div>
                                    </div>
                                    <div class="form-group"><label class="col-sm-3 control-label">Asunto:</label>
                                        <div class="col-sm-9"><input required type="text" class="form-control" name="subject" maxlength="40" minlength="5" placeholder="Ingrese el asunto" style="text-transform:uppercase;"></div>
                                    </div>


                                    <input type="hidden" value="<%= user.id%>" name="user">


                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group">
                                    <div class="col-sm-12">
                                        <textarea required class="form-control summernote" name="description" id="description" cols="30" rows="10"></textarea>
                                    </div>
                                </div>
                            </div>



                        </fieldset>
           
                    </form>

                </div>

                <div class="modal-footer">
                    <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                    <button id="saveTicket" type="button" class="btn btn-primary">Save changes</button>

                </div>
            </div>
        </div>

    </div>

    <div class="modal fade" id="modalIns" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content modal-lg">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h4 class="modal-title">Ticket #</h4>

                </div>
                <div class="modal-body">



                    <div class="row">
                        <div class="form-group">

                            <div class="col-md-7">
                                <textarea class="form-control summernote2" name="description" id="description" cols="30" rows="10">
                                                    <div id="txtDesc"></div>
                                                </textarea>
                            </div>
                            <div class="col-md-5">

                                <div class="table-responsive">
                                    <table id="Incidences" class="footabled table table-stripped toggle-arrow-tiny" data-page-size="10">
                                        <thead>
                                            <tr>
                                                <th data-hide="true">Creado por:</th>
                                                <th data-hide="phone">Fecha</th>
                                                <th data-hide="phone">Hora</th>
                                                <th data-hide="all">Description</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr w3-repeat="incidences">
                                                <td>{{user}}</td>
                                                <td>{{date}}</td>
                                                <td>{{time}}</td>
                                                <td> {{description}}</td>
                                            </tr>
                                        </tbody>
                                        <tfoot>
                                            <tr>
                                                <td colspan="6">
                                                    <ul class="pagination pull-right"></ul>
                                                </td>
                                            </tr>
                                        </tfoot>
                                    </table>
                                </div>



                                <p>Agregar una nueva incidencia.</p>

                                <form id="formIncidence">
                                    <input type="hidden" name="user" value="<%=user.id%>">
                                    <input id="txtTicket" type="hidden" name="ticket" value="">

                                    <div class="form-group">

                                        <textarea autofocus class="form-control" name="description" placeholder="Escriba aqu√≠" id=""></textarea>
                                    </div>

                                    <div>
                                        <button id="saveIncidence" class="btn btn-sm btn-primary pull-right m-t-n-xs" type="button"><strong>Enviar</strong></button>
                                        <label> <input type="checkbox" name="isClosed" class="i-checks"> Finalizar ticket </label>
                                    </div>

                                </form>




                            </div>
                        </div>
                    </div>



                </div>

                <div class="modal-footer">
                    <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                    <button id="saveTicket2" type="button" class="btn btn-primary">Save changes</button>
                </div>
            </div>
        </div>
    </div>


